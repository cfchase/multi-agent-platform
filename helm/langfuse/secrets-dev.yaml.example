# Langfuse Secrets for Development
# Copy to secrets-dev.yaml and fill in values (or let make deploy auto-generate)
# DO NOT commit secrets-dev.yaml to git!
#
# NOTE: PostgreSQL uses the existing postgres-secret from Kustomize deployment

# Redis (deployed via subchart)
redis:
  auth:
    password: ""  # Generate: python3 -c "import secrets; print(secrets.token_urlsafe(16))"

# ClickHouse (deployed via subchart)
clickhouse:
  auth:
    password: ""  # Generate: python3 -c "import secrets; print(secrets.token_urlsafe(16))"

# MinIO/S3 (deployed via subchart)
s3:
  accessKeyId:
    value: "minio"
  secretAccessKey:
    value: ""  # Generate: python3 -c "import secrets; print(secrets.token_urlsafe(16))"

# Langfuse application secrets (nextauth is under langfuse)
langfuse:
  salt:
    value: ""  # Generate: python3 -c "import secrets; print(secrets.token_urlsafe(32))"
  encryptionKey:
    value: ""  # Generate: python3 -c "import secrets; print(secrets.token_hex(32))"
  nextauth:
    url: ""  # Your OpenShift route URL, e.g., https://langfuse-deep-research-dev.apps.your-cluster.com
    secret:
      value: ""  # Generate: python3 -c "import secrets; print(secrets.token_urlsafe(32))"
